@model IEnumerable<PastebookWebApplication.Models.PostModel>

TIMELINE
<table class="table">
    <tr>
        <th>PostId</th>
        <th>Content</th>
        <th>CreatedDate</th>
        <th>PosterId</th>
        <th>ProfileOwnerId</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.PostId</td>
            <td>@item.Content</td>
            <td>@item.CreatedDate</td>
            <td>@item.PosterId</td>
            <td>@item.ProfileOwnerId</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }

</table>
